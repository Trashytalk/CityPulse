{
  "name": "@citypulse/shared",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./validators": {
      "import": "./dist/validators/index.mjs",
      "require": "./dist/validators/index.js",
      "types": "./dist/validators/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.mjs",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/types/index.ts src/validators/index.ts src/constants/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/types/index.ts src/validators/index.ts src/constants/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@citypulse/typescript-config": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
